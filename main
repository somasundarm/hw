pipeline {
    agent any
    stages {
        stage('Helloe World') {
            steps {
                sh 'echo "Hello World "'
                  }
            }        
     stage('Do Something') {
    steps {
       sh 'echo "Doing Something"'
    }
           }
stage('Done')  {  
    steps {
         sh 'echo "All Done"'
    }
            }
    }
}
